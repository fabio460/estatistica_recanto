// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Temporada {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    data DateTime
    rodada Int
    campeao String
    artilheiro String
    assistecia String
    gols Int
    vitorias Int
    empates Int 
}
